{
    "name": "confluence-to-markdown",
    "version": "2.4.0",
    "description": "Confluence pages to Markdown exporter with diagram conversion",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build:ext": "vite build --config vite.config.extension.ts && vite build --config vite.config.background.ts && node scripts/copy-assets.js",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@whitebite/diagram-converter": "^0.1.1",
        "jszip": "^3.10.1",
        "turndown": "^7.2.0",
        "turndown-plugin-gfm": "^1.0.2"
    },
    "devDependencies": {
        "@types/chrome": "^0.0.268",
        "@types/turndown": "^5.0.5",
        "jsdom": "^24.0.0",
        "typescript": "^5.3.3",
        "vite": "^5.4.11",
        "vite-plugin-monkey": "^4.0.6",
        "vitest": "^1.6.0"
    }
}