  var spaceList287 = opt_data.spaces;
  var spaceListLen287 = spaceList287.length;
  for (var spaceIndex287 = 0; spaceIndex287 < spaceListLen287; spaceIndex287++) {
    var spaceData287 = spaceList287[spaceIndex287];
    output += '<li><span class="caption">' + soy.$$escapeHtml(spaceData287.key) + ' - ' + soy.$$escapeHtml(spaceData287.name) + '</span></li>';
  }
