  for (var diagramIndex233 = 0; diagramIndex233 < diagramListLen233; diagramIndex233++) {
    var diagramData233 = diagramList233[diagramIndex233];
    output += '<li class="attached-file" data-url="' + soy.$$escapeHtml(diagramData233.url) + '" data-name="' + soy.$$escapeHtml(diagramData233.name) + '" data-ceo-id="' + soy.$$escapeHtml(diagramData233.ceoId) + '" data-revision="' + soy.$$escapeHtml(diagramData233.revision) + '" data-mime-type="' + soy.$$escapeHtml(diagramData233.mimeType) + '" title="' + soy.$$escapeHtml('\u041f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0441\u0442\u0432\u043e') + ' - ' + soy.$$escapeHtml(diagramData233.spaceName) + '<br>' + soy.$$escapeHtml('\u041e\u0431\u043d\u043e\u0432\u043b\u0435\u043d\u043e') + ' - ' + soy.$$escapeHtml(diagramData233.lastModified) + '<br>' + soy.$$escapeHtml('\u041f\u043e\u0441\u043b\u0435\u0434\u043d\u0435\u0435 \u043e\u0431\u043d\u043e\u0432\u043b\u0435\u043d\u0438\u0435') + ' - ' + soy.$$escapeHtml(diagramData233.lastModifierName) + '<br>' + soy.$$escapeHtml('\u0412\u0435\u0440\u0441\u0438\u044f') + ' - ' + soy.$$escapeHtml(diagramData233.revision) + '"><div class="file-container" ><img class="thumbnail" src="' + soy.$$escapeHtml(diagramData233.previewUrl) + '"><i data-action="select" class="abs-icon select">Select</i>' + ((diagramData233.mimeType == 'application/gliffy+json') ? '<i class="abs-icon drawio-gliffy-logo">Zoom</i>' : '<i data-action="zoom" class="abs-icon zoom">Zoom</i>') + '</div><span class="caption filename">' + soy.$$escapeHtml(diagramData233.name) + '</span></li>';
  }
  output += '</ul></div>';
  return output;
};
